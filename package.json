{
  "name": "django-helper",
  "displayName": "Django Helper",
  "description": "Build Django projects happily.",
  "version": "0.0.1",
  "author": {
    "name": "Harshil Patel",
    "url": "https://github.com/HarshilPatel007"
  },
  "publisher": "Harshil Patel",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarshilPatel007/Django-Helper.git"
  },
  "readme": "./README.md",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Programming Languages",
    "Formatters"
  ],
  "tags": [
    "python",
    "django",
    "django-framework",
    "djago-rest-framework",
    "configuration",
    "task",
    "tasks"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "django-helper.setupVSCodeForDjangoProject",
        "title": "setup vscode for django project"
      },
      {
        "command": "django-helper.admin_register_all_models",
        "title": "register all models in admin.py"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == python",
          "command": "django-helper.admin_register_all_models",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  }
}
